    "@perawallet/connect": "^1.2.3",
    "@walletconnect/modal-sign-html": "^2.5.6",
    "algo-msgpack-with-bigint": "^2.1.1",
    "algosdk": "^2.7.0",
    "buffer": "5.0.0",
    "encrypt-with-password": "^1.0.0",
    "eslint": "8.15.0",
    "file-saver": "^2.0.5",
    "hi-base32": "^0.5.1",
    "regenerator-runtime": "^0.13.9",
    "serve": "^13.0.2",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@types/mime": "^2.0.0",
    "eslint": "8.15.0",
    "tsup": "8.0.2",
    "typescript": "5.4.3"
  }
