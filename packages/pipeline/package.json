  "keywords": [
    "Algorand",
    "Blockchain",
    "JavaScript",
    "Class"
  ],
  "author": "Headline",
  "dependencies": {
    "@agoralabs-sh/algorand-provider": "^1.1.0",
    "@json-rpc-tools/utils": "^1.7.6",
